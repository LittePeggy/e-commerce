.container{
    display: flex;
    flex-direction: column;
    align-items: center;
    background-color: #ffffff;
    padding-top: 40rpx;
}

.title-img{
    width: 690rpx;
    height: 90rpx;
}

.inner{
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: space-between;
    margin-top: 32rpx;
}

.left{
    display: flex;
    height: 522rpx;
    width: 346rpx;
    margin-left: 0;
}

.right-inner{
    display: flex;
    flex-direction: column;
    margin-right: 0;
    justify-content: space-between;
}

.right-img{
    display: flex;
    width: 400rpx;
    height: 260rpx;
}
